{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"module": "ES2020",
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$stores": ["src/stores"],
			"$stores/*": ["src/stores/*"]
		},
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	},
	"ts-node": {
		"esm": true
	}
}
